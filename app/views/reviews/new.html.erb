<div class="row mt-4">
    <div class="card-header bg-primary text-white">
        <h1>New Review</h1>
        <%= simple_form_for [@restaurant, @review] do |f| %>
            <%= f.input :content%>
            <%= f.input :rating%>
            <%= f.submit "Submit Review", class: "btn-primary"%>
        <%end%>
    </div>
</div>
